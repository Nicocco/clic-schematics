{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "component": {
      "description": "clic component",
      "factory": "./component/index#component",
      "schema": "./component/schema.json",
      "aliases": ["compo"]
    },
    "service": {
      "description": "clic service",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "material-module": {
      "description": "clic material-module based on angular-material 8.2.3",
      "factory": "./material-module/index#module",
      "schema": "./material-module/schema.json"
    },
    "flux": {
      "description": "clic custom flux dispatcher, store and enum of actions",
      "factory": "./flux/index#flux",
      "schema": "./flux/schema.json"
    }
  }
}
